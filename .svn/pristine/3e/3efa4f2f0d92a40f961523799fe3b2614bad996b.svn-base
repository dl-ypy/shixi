table.grid {
	border-collapse: collapse;
	width: 100%
}

table.grid thead, table.grid thead th {
	border-bottom: 2px solid #d61821;
	font-weight: bold;
	padding: 3px 0;
	font-size: 12px;
	text-align: center
}

table.grid tbody td, table.grid tbody tr {
	border-bottom: 1px solid #ccc
}

table.grid td {
	padding: 3px;
	font-size: 12px
}

table.grid tbody tr .operation-data-row a {
	visibility: hidden;
	filter: alpha(opacity = 0);
	-moz-opacity: 0;
	opacity: 0
}

table.grid tbody tr:hover .operation-data-row a {
	visibility: visible;
	filter: alpha(opacity = 80);
	-moz-opacity: .8;
	opacity: .8
}

table.grid .grid-row-number {
	color: #828282
}

table.grid .even {
	background-color: #f2f2f2
}

table.grid tbody tr:hover {
	background-color: #ffc
}

table.grid .string, table.grid .left {
	text-align: left
}

table.grid .number, table.grid .right {
	text-align: right
}

table.grid .date, table.grid .center {
	text-align: center
}

table.grid td a {
	marggin: 0 3px
}

td.postTd {
	background: url("images/menu/red.gif") no-repeat left center transparent
}

input.submit, input.button, input.cancel, button.button, button.submit,
	button.cancel {
	background: url(images/buttons.png) no-repeat scroll 0 -50px transparent;
	border: none;
	cursor: pointer;
	height: 25px;
	text-align: center;
	width: 80px
}

input.submit:hover, input.button:hover, input.cancel:cancel, button.button:hover,
	button.submit:hover, button.cancel:hover {
	text-decoration: underline
}

.pop-menu ul li a.lvl0 {
	border: none;
	height: 38px;
	line-height: 38px;
	color: #444;
	margin: 1px 1px 0 1px
}

.pop-menu ul li:hover a.lvl0 {
	border: 1px solid #8b0000;
	border-bottom: 0;
	margin: 0
}

.pop-menu ul li:hover div table td {
	border-left: 1px solid #d2a7a7
}

.pop-menu ul li:hover div {
	border: 1px solid #8b0000;
	border-top: 0
}

.pop-menu ul li:hover div ul li a:hover {
	color: #8b0000 !important
}

.slide-menu ul li a.lvl0 {
	height: 38px;
	line-height: 38px;
	color: #444
}

.slide-menu ul li ul li a.collapsed:hover {
	background-color: #8b0000
}

.slide-menu ul li ul li a:hover {
	background-color: #8b0000;
	color: #fff !important
}

.slide-menu ul li ul li ul {
	border: 1px solid #8b0000
}

