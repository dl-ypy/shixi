@default-tree-width: 320px;

.tree-container {
  position: fixed;
  top: 80px;
  bottom: 12px;
  width: @default-tree-width;
  z-index:1009;
  background-color:#FCFCFC;
  box-shadow:0 2px 5px rgba(0,0,0,0.26);
  border-radius: 5px;
  overflow: hidden;

  &.has-toolbar {
    padding:48px 0 0 0;
    
      & > .tree-toolbar {
        position:absolute;
        top:0;
        width:100%;
        height:46px;
        background-color:#009688;
        box-shadow:0 2px 5px rgba(0,0,0,0.26);
        z-index:1;
        
          & > * {
            margin:8px;
            float:left;
          }
      
          & > .title {
            margin-left:16px;
            line-height:32px;
            font-size: 16px;
            color:#FFF;
          }

          div.select {
            margin-left: 48px;
            padding: 0 6px;
            float: none;
            
            select {
              width: 100%;
              margin-left: 0;
            }
          }
          
          .hamburger {
            margin: 8px;
            font-size: 16px;
            line-height: 32px;
          }
        
          & > .tree-toolbar-icon {
            margin:16px;
            font-size: 18px;
            float:right;
            cursor: pointer;
          }

          select {
            width: 240px;
            height: 28px;
            margin-left:5px;
            font-size:16px;
            color:#FFF;
            background-color:#009688;
            border:none;
          }
      }
    
      & > .tree-box {
        width:100%;
        height:100%;
        overflow: auto;
      }
      & > .tree-box::-webkit-scrollbar {
        width: 7px;
        height: 8px;
      }
      & > .tree-box::-webkit-scrollbar-thumb {
        border-radius: 5px;
        background: rgba(0,0,0,0.08);
        -webkit-box-shadow: inset 0 0 2px rgba(0,0,0,0.1);
      }
  }

}

.black-cover {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color:rgba(0,0,0,0.5);
  opacity:0;
  z-index:-1;
}

.page-container {
	padding-left: @default-tree-width + 15;
    overflow: hidden;
    
    .content {
        position: relative;
        left: 0;
        right: 0;
        bottom: 0;
        top: 0;
    }
}

#jstree-marker, .jstree-contextmenu {
  z-index:1009;
}

.bonc-tree-icon {
  background-image: url(data:image/png;base64,) !important;

  &.root {
    background-position:-64px 0 !important;
  }
  &.default {
    background-position:0 0 !important;
  }
  &.leaf {
    background-position:-32px 0 !important;
  }
}