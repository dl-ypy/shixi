		          FROM CW_DM.CW_RPT_ZTDB_BSDB_2_1 T
		         WHERE T.MONTH_ID LIKE '$year$%'
		           AND T.MONTH_ID <![CDATA[ <= ]]> #monthId#
		           AND T.ORG_ID = #orgId#) B
