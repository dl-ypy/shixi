<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="commons-default" namespace="/" extends="struts-default">
		<global-results>
			<!-- 跳转到指定的url上-->
			<result name="gourl" type="redirect">${gourl}</result>
		</global-results>

		<!-- SelectListTag（HTML联动框）使用 -->
		<action name="SelectList" class="com.bonc.tags.html.components.SelectListAction" />

		<!-- webChart展示使用-->
		<action name="Chart" class="com.bonc.tags.chart.webcharts.ChartAction" />
	</package>
</struts>
