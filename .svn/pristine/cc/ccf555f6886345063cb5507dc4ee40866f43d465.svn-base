#mkrTMDiv {
	width: 136px;
	height: 28px;
	position: absolute;
	left: 160px;
	top: 60px;
	display: none;
	z-index: 200;
	border: solid 1px gray;
	background: #ffffff;
}

#TMDivs {
	width: 280px;
	height: 160px;
	border: solid 1px gray;
	position: absolute;
	left: 160px;
	top: 90px;
	margin: 2px; 0 px;
	display: none;
	z-index: 201;
}

#TMDivs ul {
	list-style-type: none;
	padding: 2px 2px;
	margin: 2px 2px;
}

#TMDivs ul li a {
	cursor: pointer;
	margin: 2px 2px;
	width: 30px;
	height: 30px;
	display: inline-block;
	border: solid 1px #ffffff;
	text-align: center;
}

#TMDivs ul li a:hover {
	background: none;
	border: solid 1px gray;
}

#TMDivs ul li img {
	border: none;
	background:
		url('http://api.map.baidu.com/library/MarkerTool/1.2/examples/images/icon.gif');
}

