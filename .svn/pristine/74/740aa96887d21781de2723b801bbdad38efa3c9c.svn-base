	<select id="queryChart" resultClass="java.util.HashMap">
	SELECT A.MONTH_ID, B.KPI_01,B.KPI_06
			FROM HN_DMCODE.CODE_MONTH A,
			(SELECT
			SUBSTR(T.MONTH_ID, 5) MONTH_ID, NVL(T.KPI_01, 0) KPI_01,NVL(T.KPI_06, 0) KPI_06
