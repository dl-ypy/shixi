	<!-- 收入利润预算完成 -->
   	<select id="BszxdbSrlrysTable1" resultClass="java.util.HashMap">
	SELECT T.MONTH_ID, A.ORG_ID, A.ORG_NAME,
	       T.KPI_01, T.KPI_02, T.KPI_03, T.KPI_04, T.KPI_05, T.KPI_06,
