	  FROM CW_DM.CW_RPT_ZTDB_BSDB_2_1 T, HN_DMCODE.DMCODE_PRO_CW_VIEW A
	 WHERE T.MONTH_ID = #monthId#
	   AND T.ORG_ID = A.ORG_ID
	 ORDER BY A.ORD
    </select>
   	<select id="BszxdbSrlrysLine1" resultClass="java.util.HashMap">
